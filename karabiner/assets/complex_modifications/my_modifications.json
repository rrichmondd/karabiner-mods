{
    "title": " ~ Richmond ~ ",
    "rules": [
        {
            "description": "Tab Modifier", 
            "manipulators": [
                {
                    "description": "[Q] -> [left_shift]",
                    "type": "basic",
                    "conditions": [
                        {
                            "name": "holdTab",
                            "type": "variable_if",
                            "value": 1
                        }
                    ],
                    "from": {
                        "key_code": "q",
                        "modifiers": {
                            "optional": ["any"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "left_shift"
                        }
                    ]
                },
                {
                    "description": "[W] -> [left_option]",
                    "type": "basic",
                    "conditions": [
                        {
                            "name": "holdTab",
                            "type": "variable_if",
                            "value": 1
                        }
                    ],
                    "from": {
                        "key_code": "w",
                        "modifiers": {
                            "optional": ["any"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "left_option"
                        }
                    ]
                },
                {
                    "description": "[E] -> [left_command] ",
                    "type": "basic",
                    "conditions": [
                        {
                            "name": "holdTab",
                            "type": "variable_if",
                            "value": 1
                        }
                    ],
                    "from": {
                        "key_code": "e",
                        "modifiers": {
                            "optional": ["any"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "left_command"
                        }
                    ]
                },
                {
                    "description": "[F] -> [FN]",
                    "type": "basic",
                    "conditions": [
                        {
                            "name": "holdTab",
                            "type": "variable_if",
                            "value": 1
                        }
                    ],
                    "from": {
                        "key_code": "f",
                        "modifiers": {
                            "optional": ["any"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "fn"
                        }
                    ]
                },
                {
                    "description": "H to left_arrow",
                    "type": "basic",
                    "conditions": [
                        {
                            "name": "holdTab",
                            "type": "variable_if",
                            "value": 1
                        }
                    ],
                    "from": {
                        "key_code": "h",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "left_arrow"
                        }
                    ]
                },
                {
                    "description": "J to down_arrow",
                    "type": "basic",
                    "conditions": [
                        {
                            "name": "holdTab",
                            "type": "variable_if",
                            "value": 1
                        }
                    ],
                    "from": {
                        "key_code": "j",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "down_arrow"
                        }
                    ]
                },
                {
                    "description": "K to up_arrow",
                    "type": "basic",
                    "conditions": [
                        {
                            "name": "holdTab",
                            "type": "variable_if",
                            "value": 1
                        }
                    ],
                    "from": {
                        "key_code": "k",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "up_arrow"
                        }
                    ]
                },
                {
                    "description": "L to right_arrow",
                    "type": "basic",
                    "conditions": [
                        {
                            "name": "holdTab",
                            "type": "variable_if",
                            "value": 1
                        }
                    ],
                    "from": {
                        "key_code": "l",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "right_arrow"
                        }
                    ]
                },
                {
                    "description": "[Spacebar] -> [Backspace]",
                    "type": "basic",
                    "conditions": [
                        {
                            "name": "holdTab",
                            "type": "variable_if",
                            "value": 1
                        }
                    ],
                    "from": {
                        "key_code": "spacebar",
                        "modifiers": {
                            "optional": ["any"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "delete_or_backspace"
                        }
                    ]
                },
                {
                    "description": "[Tab] -> [Modifier]",
                    "type": "basic",
                    "from": {
                        "key_code": "tab",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "set_variable": {
                                "name": "holdTab",
                                "value": 1
                            }
                        }
                    ],
                    "to_after_key_up": [
                        {
                            "set_variable": {
                                "name": "holdTab",
                                "value": 0
                            }
                        }
                    ],
                    "to_if_alone": [
                        {
                            "key_code": "tab"
                        }
                    ]
                }
            ]
        },
        {
            "description": "Simultaneous",
            "manipulators": [
                {
                    "description": "[Q][W] -> [Escape]",
                    "type": "basic",
                    "parameters": {
                        "basic.simultaneous_threshold_milliseconds": 1000 
                    },
                    "from": {
                        "simultaneous": [
                            {
                                "key_code": "q"
                            },
                            {
                                "key_code": "w"
                            }
                        ],
                        "simultaneous_options": {
                            "key_down_order": "strict",
                            "to_after_key_up": [
                               {
                                    "set_variable": {
                                        "name": "simultaneousVariable",
                                        "value": 0
                                    }
                                }
                            ]
                        },
                        "modifiers": {
                            "optional": ["any"]
                        }
                    },
                    "to": [
                        {
                            "set_variable": {
                                "name":"simultaneousVariable",
                                "value": 1
                            }
                        },
                        {
                            "key_code": "escape"
                        }
                    ]
                }
            ]
        },
        {
            "description": "Remap Modifiers",
            "manipulators": [
                {
                    "description": "[CapsLock] -> [left_Shift] ",
                    "type": "basic",
                    "from": {
                        "key_code": "caps_lock",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "left_shift"
                        }
                    ],
                    "parameters": {
                        "basic.to_if_alone_timeout_milliseconds": 300
                    }
                },
                {
                    "description": "[;] -> [right_shift]",
                    "type": "basic",
                    "from": {
                        "key_code": "semicolon",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "right_shift"
                        }
                    ],
                    "to_if_alone": [
                        {
                            "key_code": "semicolon"
                        }
                    ],
                    "parameters": {
                        "basic.to_if_alone_timeout_milliseconds": 300
                    }
                },
                {
                    "description": "['] -> [right_control]",
                    "type": "basic",
                    "from": {
                        "key_code": "quote",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "right_control"
                        }
                    ],
                    "to_if_alone": [
                        {
                            "key_code": "quote"
                        }
                    ],
                    "parameters": {
                        "basic.to_if_alone_timeout_milliseconds": 300
                    }
                },
                {
                    "description": "[slash] -> [fn]",
                    "type": "basic",
                    "from": {
                        "key_code": "slash",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "fn"
                        }
                    ],
                    "to_if_alone": [
                        {
                            "key_code": "slash"
                        }
                    ],
                    "parameters": {
                        "basic.to_if_alone_timeout_milliseconds": 300
                    }
                }
            ]
        }
    ]
}
